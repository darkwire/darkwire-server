  "dependencies": {
    "kcors": "2",
    "koa": "^2.3.0",
    "koa-body": "^2.3.0",
    "koa-router": "^7.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "socket.io": "^2.0.3"
  },
  "scripts": {
    "build": "rm -rf build && rm -rf dist && babel src -d dist/src && mkdir build && zip -r build/build.zip . -x \"src\" -x \".git\"",
    "dev": "nodemon src/index.js --exec babel-node",
    "test": "jest",
    "test:watch": "jest --watch",
    "start": "node dist/src/index.js",
    "predeploy": "npm run build",
    "deploy": "eb deploy --staged"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.6.1",
    "jest": "^21.0.2",
    "nodemon": "^1.12.0"
  }
